<h1> Category Details </h1>
<p id="notice"><%= notice %></p>
<table>
  <% @category.each do |category| %>
    <tr>
        <td><b>Category name: </b></td>
        <td><%= category.name %></td>
    </tr>
    <tr>
        <td><a href="/category/<%= category.id %>/edit">Edits</a></td>
        <td><%= link_to 'Back', category_index_path %></td>
    </tr>
  <% end %>
</table>